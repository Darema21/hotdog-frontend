.tab-bar {
  /* height: 128rpx;
  /* position: absolute; */
  /* bottom: 10%; */
  /* left: 0; */
  /* right: 0;  */
  background: white;
  display: flex;
  padding: 16rpx 0 0 0 ;
  padding-bottom: env(safe-area-inset-bottom);
  box-shadow: 0px 3px 24px 0px rgba(92, 91, 91, 0.15);
  pointer-events: auto;
  border-top-left-radius: 40rpx; /* Adjust the value as needed */
  border-top-right-radius: 40rpx; /* Adjust the value as needed */
}


.tab-bar-item {
    flex: 1;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.tab-bar-item cover-image {
    width: 48px;
    height: 48px;
}

.tab-bar-item cover-view {
    font-size: 10px;
}